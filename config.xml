<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.app.bmi"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>App BMI</name>

  <description>
    App per il calcolo del BMI dati il peso in KG, l'altezza in metri, il sesso.
  </description>

  <author  email="attoboatchie.riccardo@canossamilano.org">
	Attboatchie Ange
  </author>

	<platform name="android" />
        <icon src="BMI.jpg" platform="android" qualifier="ldpi" />
        <icon src="images.jpeg" platform="android" qualifier="mdpi" />
        <icon src="hdpi.png" platform="android" qualifier="hdpi" />

	<access origin="*"/>
	<plugin name="cordova-plugin-whitelist" source="npm" />
	<allow-navigation href="*://*getskeleton.com/*" />
</widget>
